<script type="text/javascript">
  var waitForJQuery = setInterval(function () {
  	if (typeof $ != 'undefined') {
  		console.log('loaded jquery')
  		// place your code here.
  		clearInterval(waitForJQuery);
  		initForm();
  	} else {
  		console.log('waiting for jquery')
  	}
  }, 500);

  function initForm() {
  	$(document).ready(function(){
      var submitErrorMsg = "There is a problem with the form, please check and correct errors.";

      if ($('.js-sailthru-birthday').length) {
         var cleaveDate = new Cleave('.js-sailthru-birthday', {
          date: true,
          delimiter: '.',
          datePattern: ['d', 'm', 'Y']
        }); 
      }

      var getUrlParameter = function getUrlParameter(sParam) {
          var sPageURL = decodeURIComponent(window.location.search.substring(1)),
              sURLVariables = sPageURL.split('&'),
              sParameterName,
              i;
          for (i = 0; i < sURLVariables.length; i++) {
              sParameterName = sURLVariables[i].split('=');
              if (sParameterName[0] === sParam) {
                  return sParameterName[1] === undefined ? true : sParameterName[1];
              }
          }
      };

      var setImageHeight = function() {
        $('.js-sailthru-form-wrapper').each(function(){
          var $this = $(this),
              $width = $(window).outerWidth(),
              backgroundHeight = $this.data('background-height'),
              backgroundWidth = $this.data('background-width'),
              backgroundHeightMobile = $this.data('background-height-mobile'),
              backgroundWidthMobile = $this.data('background-width-mobile');

          if (backgroundHeight && backgroundWidth) {
            var imageRatio = parseInt(backgroundHeight) / parseInt(backgroundWidth),
                containerWidth = $this.outerWidth(),
                newHeight =  containerWidth * imageRatio;

            $this.css('min-height', newHeight);
          }

          if ($width < 769 && backgroundHeightMobile && backgroundWidthMobile) {
            console.log('here');
            var imageRatioMobile = parseInt(backgroundHeightMobile) / parseInt(backgroundWidthMobile),
                containerWidth = $this.outerWidth(),
                newHeightMobile = containerWidth * imageRatioMobile;

          if (newHeightMobile < 800) {
              newHeightMobile = 800;
            }
            
            $this.css('min-height', newHeightMobile);
          }
        })
      }

      setImageHeight();

      var validateForm = function() {
        var isValidForm = true;
        $('.js-sailthru-validation-error').addClass('hidden');
        
        $('.js-sailthru-form input').each(function(){
          var $this = $(this),
              name = $this.attr('name'),
              isRequired = $this.data('required'),
              value = $this.val();

          $this.next().addClass('hidden');
          
          if (isRequired && value == "") {
            $this.next().removeClass('hidden');
            isValidForm = false
          } else if (name == "email" && value.indexOf('@') < 1) {
            $this.next().html('Please enter correct email').removeClass('hidden');
            isValidForm = false;
          }
        })

        $('.js-sailthru-form select').each(function(){
          var $this = $(this),
              isRequired = $this.data('required'),
              value = $this.find(":selected").val();
          
          if (isRequired && value == "") {
            $this.next().removeClass('hidden');
            isValidForm = false;
          } else {
            $this.next().addClass('hidden');
          }
        })

        return isValidForm;
      }

      
      $('.js-sailthru-form').on('submit', function(e){
        e.preventDefault();
        var $form = $(this),
            email = $form.find('.js-sailthru-email').val(),
            firstName = $form.find('.js-sailthru-firstname').val(),
            birthday = $form.find('.js-sailthru-birthday').val(),
            phone = $form.find('.js-sailthru-phone').val(),
            facebook = $form.find('.js-sailthru-facebook').val(),
            twitter = $form.find('.js-sailthru-twitter').val(),
            tiktok = $form.find('.js-sailthru-tiktok').val(),
            instagram = $form.find('.js-sailthru-instagram').val(),
            snapchat = $form.find('.js-sailthru-snapchat').val(),
            pinterest = $form.find('.js-sailthru-pinterest').val(),
            youtube = $form.find('.js-sailthru-youtube').val(),
            lastName = $form.find('.js-sailthru-lastname').val(),
            whereFrom = $form.find('.js-sailthru-where').find(":selected").val(),
            source = getUrlParameter('utm_source'),
            isValidForm = validateForm();


        if (isValidForm) {
          $form.addClass('is-loading');
          
          Sailthru.integration("userSignUp", 
          { 
          "email" : email,
          "lists" : {
            "*MASTER LIST" : 1,
            "DAILY (Main)" : 1},
          "keys": {
           "sms": phone
          },
          "optout_email": "none",  
          "fields": {
            "optout_email": "none",
          },
          "id": email,
          "key": "email",
          "vars": {
              "firstname": firstName,
              "lastname": lastName,
              "birth_date": birthday,
              "tiktok": tiktok,
              "instagram": instagram,
              "snapchat": snapchat,
              "fb": facebook,
              "twitter": twitter,
              "pinterest": pinterest,
              "youtube": youtube,
              "whereFrom": whereFrom,
              "pageUrl": window.location.pathname
          },  
          "source" : source,
          "onSuccess" : function() {
            $form.removeClass('is-loading');
            $form.find('.js-submitted-email').html(email);
            $form.find('.js-sailthru-submit-wrapper').addClass('is-success');
            $form.find('.js-sailthru-input').addClass('hidden');
            $form.find('.js-sailthru-submit-button').addClass('hidden');
            $form.find('.js-sailthru-submitted').removeClass('hidden');
            localStorage.setItem('em', email);
           },
          "onError" : function(response) {
            $form.removeClass('is-loading');
            $('.js-sailthru-validation-error').html(submitErrorMsg + ' (' + response.error + ')' ).removeClass('hidden');
           }
        });

        gtag("event", "Subscribe", {
          "Page": window.location.href.split('?')[0]
        });
                  
        } else {
          $('.js-sailthru-validation-error').html(submitErrorMsg).removeClass('hidden');
        }
      })
    })
  }
  
  
</script>




